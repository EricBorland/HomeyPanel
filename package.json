{
  "name": "homey-panel",
  "version": "0.2.0",
  "scripts": {
    "build": "ng build --prod=true",
    "e2e": "ng e2e",
    "eslint": "eslint src e2e --ext .ts --fix",
    "generate-icon-json": "cat src/assets/icons.svg | awk -F\"id=\" '{print $2}' | cut -f1 -d' ' | grep . | awk ' BEGIN { ORS = \"\"; print \"{ \\\"icons\\\": [\"; } { print $0\",\"; } END { print \"] }\"; }' | sed \"s/,]/]/\" > src/assets/icons.json",
    "generate-icon-set": "cd src/assets/icons && ls | awk '{printf(\"\\\"%s\\\"\\n\", $0)}' | sed 'p; s/\\ /-/g' | xargs -n2 mv && ls *.svg | xargs -I{} sh -c 'cat {} | sed \"s/<svg /<svg id=\\\"$PREFIX-{}\\\" /g\"' | sed \"s/.svg\\\"/\\\"/g\" | grep -v ?xml > ../icon-set.svg && rm -Rf *.svg",
    "install:hooks": ". ./hooks/install.sh",
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "test": "ng test",
    "postinstall": "npm run generate-icon-json; npm run install:hooks",
    "version": "standard-version"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "8.2.14",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/pwa": "0.803.21",
    "@angular/router": "8.2.14",
    "@angular/service-worker": "8.2.14",
    "@mdi/angular-material": "4.7.95",
    "chart.js": "2.9.3",
    "hammerjs": "2.0.8",
    "ng2-charts": "2.3.0",
    "ng2-dragula": "2.1.1",
    "rxjs": "6.4.0",
    "svgson-cli": "1.1.1",
    "tslib": "1.10.0",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.21",
    "@angular/cli": "8.3.21",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/hammerjs": "2.0.36",
    "@types/jasmine": "3.3.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "@typescript-eslint/eslint-plugin": "2.16.0",
    "@typescript-eslint/parser": "2.16.0",
    "codelyzer": "5.0.0",
    "eslint": "6.8.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "6.3.14",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "protractor": "5.4.0",
    "standard-version": "8.0.1",
    "ts-node": "7.0.0",
    "typescript": "3.5.3"
  }
}
